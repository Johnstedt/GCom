<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Text?>

<BorderPane fx:id="borderPane" minHeight="-Infinity" minWidth="-Infinity" prefWidth="450.0" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <GridPane fx:id="gridPane" hgap="10.0" minHeight="-Infinity" minWidth="-Infinity">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="230.0" minWidth="10.0" prefWidth="196.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="251.0" minWidth="10.0" prefWidth="194.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints maxHeight="193.0" minHeight="10.0" prefHeight="80.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="529.0" minHeight="10.0" prefHeight="451.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <ListView fx:id="senderAtDebug" minHeight="200.0" minWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <FlowPane fx:id="sendFlowPane" alignment="CENTER" columnHalignment="CENTER" orientation="VERTICAL" prefHeight="60.0" prefWidth="251.0" prefWrapLength="0.0" GridPane.columnIndex="1">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Sender" />
                  <Button fx:id="senderBtn" mnemonicParsing="false" text="Auto" />
                  <Button fx:id="sendFlush" disable="true" mnemonicParsing="false" text="sendFlush" />
               </children>
            </FlowPane>
            <FlowPane fx:id="recFlowPane" alignment="CENTER" columnHalignment="CENTER" orientation="VERTICAL">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Receiver" />
                  <Button fx:id="receiverBtn" mnemonicParsing="false" text="Auto" />
                  <Button fx:id="recFlush" disable="true" mnemonicParsing="false" text="recFlush" />
               </children>
            </FlowPane>
            <ListView fx:id="receiverAtDebug" minHeight="200.0" minWidth="200.0" GridPane.rowIndex="1" />
         </children>
         <BorderPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" />
         </BorderPane.margin>
      </GridPane>
   </center>
   <top>
      <FlowPane fx:id="topFields" alignment="TOP_CENTER" columnHalignment="CENTER" orientation="VERTICAL" prefWrapLength="75.0">
         <children>
            <Text fx:id="groupNameTxtField" strokeType="OUTSIDE" strokeWidth="0.0" text="Group Name" />
            <Text fx:id="orderTypeTxtField" strokeType="OUTSIDE" strokeWidth="0.0" text="Ordering Type" />
            <Text fx:id="comTypeTxtField" strokeType="OUTSIDE" strokeWidth="0.0" text="Communication Type" />
         </children>
         <BorderPane.margin>
            <Insets bottom="10.0" />
         </BorderPane.margin>
      </FlowPane>
   </top>
</BorderPane>
